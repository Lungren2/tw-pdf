{
  "name": "tw-pdf",
  "version": "0.1.2",
  "license": "MIT",
  "description": "A fork of react-pdf with Tailwind CSS integration",
  "author": "Ethan Ogle <ethan@easygames.co.za>",
  "homepage": "https://github.com/Lungren2/tw-pdf#readme",
  "type": "module",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lungren2/tw-pdf.git"
  },
  "keywords": [
    "react",
    "pdf",
    "tailwind",
    "tailwindcss",
    "react-pdf"
  ],
  "scripts": {
    "test": "vitest",
    "build": "rimraf ./lib && rollup -c",
    "watch": "rimraf ./lib && rollup -c -w",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run verify-package",
    "verify-package": "npm run build && npm pack --dry-run",
    "release": "npm run verify-package && npm publish"
  },
  "dependencies": {
    "@react-pdf/renderer": "^4.3.0",
    "@react-pdf/stylesheet": "^6.1.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/react": "^18.2.55",
    "rimraf": "^5.0.5",
    "rollup": "^4.12.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-local-resolve": "^1.0.7",
    "typescript": "^5.3.3",
    "vitest": "^3.1.1"
  },
  "files": [
    "lib"
  ]
}
